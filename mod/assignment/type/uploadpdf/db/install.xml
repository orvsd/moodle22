<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/assignment/type/uploadpdf/db" VERSION="20091201" COMMENT="XMLDB file for Moodle mod/assignment/type/uploadpdf"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="assignment_uploadpdf_comment" COMMENT="assignment_uploadpdf_comment table retrofitted from MySQL - stores comments/annotations added to pdfs submitted by students" NEXT="assignment_uploadpdf">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="assignment_submission"/>
        <FIELD NAME="assignment_submission" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="id" NEXT="posx"/>
        <FIELD NAME="posx" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="x-position of the top-left corner of the comment (in pixels - image resolution is set to 100 pixels per inch)" PREVIOUS="assignment_submission" NEXT="posy"/>
        <FIELD NAME="posy" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="y-position of the top-left corner of the comment (in pixels - image resolution is set to 100 pixels per inch)" PREVIOUS="posx" NEXT="width"/>
        <FIELD NAME="width" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="120" SEQUENCE="false" COMMENT="width, in pixels, of the comment box" PREVIOUS="posy" NEXT="rawtext"/>
        <FIELD NAME="rawtext" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" COMMENT="Raw text of the comment" PREVIOUS="width" NEXT="pageno"/>
        <FIELD NAME="pageno" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="The page in the PDF that this comment appears on" PREVIOUS="rawtext" NEXT="colour"/>
        <FIELD NAME="colour" TYPE="char" LENGTH="10" NOTNULL="false" DEFAULT="yellow" SEQUENCE="false" COMMENT="Can be red, yellow, green, blue, white, clear" PREVIOUS="pageno"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for assignment_uploadpdf_comment"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="assignment_submission" UNIQUE="false" FIELDS="assignment_submission" NEXT="assignment_submission_pageno"/>
        <INDEX NAME="assignment_submission_pageno" UNIQUE="false" FIELDS="assignment_submission, pageno" COMMENT="When searching for all comments on the current page" PREVIOUS="assignment_submission"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="assignment_uploadpdf" COMMENT="Stores extra settings for an assignment needed by uploadpdf plugin" PREVIOUS="assignment_uploadpdf_comment" NEXT="assignment_uploadpdf_tmpl">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="assignment"/>
        <FIELD NAME="assignment" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="id" NEXT="coversheet"/>
        <FIELD NAME="coversheet" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" COMMENT="Filename of the coversheet to attach" PREVIOUS="assignment" NEXT="template"/>
        <FIELD NAME="template" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="ID of the template to use when a student submits work with this coversheet" PREVIOUS="coversheet" NEXT="onlypdf"/>
        <FIELD NAME="onlypdf" TYPE="int" LENGTH="2" NOTNULL="false" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" COMMENT="Only allow PDF files to be uploaded" PREVIOUS="template" NEXT="checklist"/>
        <FIELD NAME="checklist" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="The checklist that you want to link this assignment to" PREVIOUS="onlypdf" NEXT="checklist_percent"/>
        <FIELD NAME="checklist_percent" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="What percentage complete the checklist must be to allow a student to submit their assignment" PREVIOUS="checklist"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for assignment_uploadpdf"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="assignment" UNIQUE="true" FIELDS="assignment" NEXT="template"/>
        <INDEX NAME="template" UNIQUE="false" FIELDS="template" COMMENT="Used when deleting a template" PREVIOUS="assignment"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="assignment_uploadpdf_tmpl" COMMENT="Stores the data describing a coversheet template" PREVIOUS="assignment_uploadpdf" NEXT="assignment_uploadpdf_tmplitm">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="Name that the template is known by" PREVIOUS="id" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Courseid that this template belongs to (0 for global)" PREVIOUS="name"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for assignment_uploadpdf_tmpl"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="course" UNIQUE="false" FIELDS="course"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="assignment_uploadpdf_tmplitm" COMMENT="Stores the data describing a coversheet template" PREVIOUS="assignment_uploadpdf_tmpl" NEXT="assignment_uploadpdf_qcklist">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="template"/>
        <FIELD NAME="template" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="type"/>
        <FIELD NAME="type" TYPE="char" LENGTH="15" NOTNULL="true" DEFAULT="shorttext" SEQUENCE="false" PREVIOUS="template" NEXT="xpos"/>
        <FIELD NAME="xpos" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="type" NEXT="ypos"/>
        <FIELD NAME="ypos" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="xpos" NEXT="width"/>
        <FIELD NAME="width" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="ypos" NEXT="setting"/>
        <FIELD NAME="setting" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="width"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for assignment_uploadpdf_tmplitm"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="template" UNIQUE="false" FIELDS="template"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="assignment_uploadpdf_qcklist" COMMENT="Stores teacher specified quicklist comments" PREVIOUS="assignment_uploadpdf_tmplitm" NEXT="assignment_uploadpdf_annot">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="text"/>
        <FIELD NAME="text" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" PREVIOUS="userid" NEXT="width"/>
        <FIELD NAME="width" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="text" NEXT="colour"/>
        <FIELD NAME="colour" TYPE="char" LENGTH="10" NOTNULL="true" DEFAULT="yellow" SEQUENCE="false" PREVIOUS="width"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for assignment_uploadpdf_qcklist"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="userid" UNIQUE="false" FIELDS="userid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="assignment_uploadpdf_annot" COMMENT="stores annotations added to pdfs submitted by students" PREVIOUS="assignment_uploadpdf_qcklist">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="assignment_submission"/>
        <FIELD NAME="assignment_submission" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="id" NEXT="startx"/>
        <FIELD NAME="startx" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="x-position of the start of the annotation (in pixels - image resolution is set to 100 pixels per inch)" PREVIOUS="assignment_submission" NEXT="starty"/>
        <FIELD NAME="starty" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="y-position of the start of the annotation (in pixels - image resolution is set to 100 pixels per inch)" PREVIOUS="startx" NEXT="endx"/>
        <FIELD NAME="endx" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="x-position of the end of the annotation" PREVIOUS="starty" NEXT="endy"/>
        <FIELD NAME="endy" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="y-position of the end of the annotation" PREVIOUS="endx" NEXT="path"/>
        <FIELD NAME="path" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" COMMENT="SVG path describing the freehand line" PREVIOUS="endy" NEXT="pageno"/>
	<FIELD NAME="pageno" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="The page in the PDF that this annotation appears on" PREVIOUS="path" NEXT="colour"/>
        <FIELD NAME="colour" TYPE="char" LENGTH="10" NOTNULL="false" DEFAULT="red" SEQUENCE="false" COMMENT="Can be red, yellow, green, blue, white, black" PREVIOUS="pageno" NEXT="type"/>
        <FIELD NAME="type" TYPE="char" LENGTH="10" NOTNULL="false" DEFAULT="line" SEQUENCE="false" COMMENT="Currently can only be 'line' - present to allow for future development" PREVIOUS="colour"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for assignment_uploadpdf_annot"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="assignment_submission" UNIQUE="false" FIELDS="assignment_submission" NEXT="assignment_submission_pageno"/>
        <INDEX NAME="assignment_submission_pageno" UNIQUE="false" FIELDS="assignment_submission, pageno" COMMENT="When searching for all annotations on the current page" PREVIOUS="assignment_submission"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
